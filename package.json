{
	"bugs": "https://github.com/cedx/lcov.js/issues",
	"description": "Parse and format to LCOV your code coverage reports.",
	"homepage": "https://github.com/cedx/lcov.js",
	"license": "MIT",
	"name": "@cedx/lcov",
	"repository": "cedx/lcov.js",
	"type": "module",
	"version": "8.1.0",
	"author": {
		"email": "cedric@belin.io",
		"name": "CÃ©dric Belin",
		"url": "https://belin.io"
	},
	"devDependencies": {
		"@types/node": "^20.11.10",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"del": "^7.1.0",
		"eslint": "^8.56.0",
		"execa": "^8.0.1",
		"typedoc": "^0.25.7",
		"typescript": "^5.3.3"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"exports": {
		"types": "./lib/index.d.ts",
		"import": "./lib/index.js"
	},
	"files": [
		"lib/",
		"src/"
	],
	"imports": {
		"#lcov": {
			"types": "./lib/index.d.ts",
			"import": "./lib/index.js"
		}
	},
	"keywords": [
		"coverage",
		"formatter",
		"lcov",
		"parser",
		"test",
		"writer"
	],
	"scripts": {
		"build": "tsc --project src/tsconfig.json",
		"clean": "node tool/clean.js",
		"dist": "npm run clean && npm run build",
		"doc": "typedoc --options etc/typedoc.js && node tool/doc.js",
		"lint": "tsc --project tsconfig.json && eslint --config=etc/eslint.cjs example src test tool",
		"postpublish": "node tool/publish.js",
		"prepack": "npm run dist",
		"test": "npm run build && node --test --test-reporter=spec"
	}
}
