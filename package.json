{
	"bugs": "https://github.com/cedx/lcov.js/issues",
	"description": "Parse and format to LCOV your code coverage reports.",
	"homepage": "https://docs.belin.io/lcov.js",
	"license": "MIT",
	"name": "@cedx/lcov",
	"repository": "cedx/lcov.js",
	"type": "module",
	"version": "8.0.1",
	"author": {
		"email": "cedric@belin.io",
		"name": "CÃ©dric Belin",
		"url": "https://belin.io"
	},
	"devDependencies": {
		"@types/node": "^20.8.6",
		"c8": "^8.0.1",
		"eslint": "^8.51.0",
		"typedoc": "^0.25.2",
		"typescript": "^5.2.2"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"exports": {
		"types": "./lib/index.d.ts",
		"import": "./src/index.js"
	},
	"files": [
		"lib/",
		"src/"
	],
	"imports": {
		"#lcov": {
			"types": "./lib/index.d.ts",
			"import": "./src/index.js"
		}
	},
	"keywords": [
		"coverage",
		"formatter",
		"lcov",
		"parser",
		"test",
		"writer"
	],
	"scripts": {
		"build": "tsc --project src/jsconfig.json",
		"clean": "node scripts/clean.js",
		"doc": "typedoc --options etc/typedoc.json && node scripts/doc.js",
		"dist": "npm run clean && npm run build",
		"lint": "tsc --project jsconfig.json && eslint --config=etc/eslint.json example scripts src test",
		"postpublish": "node scripts/publish.js",
		"prepack": "npm run dist",
		"test": "c8 --all --include=src/**/*.js --report-dir=var --reporter=lcovonly node --test --test-reporter=spec"
	}
}
